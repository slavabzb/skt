<?xml version="1.0"?>
<config>
	<service>
	</service>
	<logger>
		<sink>
			<filename>@LOG_DIR/stockviewer.log</filename>
			<level>DEBUG</level>
			<format>%(asctime)s [%(levelname)s] %(message)s</format>
			<dateformat>%d-%m-%Y %H:%M:%S</dateformat>
		</sink>
		<sink>
			<level>ERROR</level>
			<format>%(asctime)s [%(levelname)s] %(message)s</format>
			<dateformat>%d-%m-%Y %H:%M:%S</dateformat>
		</sink>
	</logger>
</config>
