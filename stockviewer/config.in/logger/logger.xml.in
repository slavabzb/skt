<?xml version="1.0"?>
<logger>
	<sink>
		<filename>@LOG/stockviewer.log</filename>
		<level>DEBUG</level>
		<format>%(asctime)s [%(levelname)s] %(message)s</format>
		<dateformat>%d-%m-%Y %H:%M:%S</dateformat>
	</sink>
	<sink>
		<level>ERROR</level>
		<format>%(message)s</format>
	</sink>
</logger>
